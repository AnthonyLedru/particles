<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="particles.ParticlesController">
   <children>
      <Canvas fx:id="canvas" height="400.0" width="400.0" />
      <Label fx:id="label" alignment="BOTTOM_RIGHT" text=" " textFill="WHITE" StackPane.alignment="BOTTOM_RIGHT">
         <font>
            <Font size="20.0" />
         </font></Label>
      <Button alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#onAdd" text="+" textFill="WHITE" StackPane.alignment="BOTTOM_LEFT" />
   </children>
</StackPane>
